ox/standard',\n    center: [coords.lng, coords.lat],\n    zoom: 17,\n    pitch: 60,\n    bearing: -20,\n    antialias: true\n  });\n\n  window.map4 = map;\n\n  map.on('load', () => {\n    const point = map.project([coords.lng, coords.lat]);\n\n    // Créer une bbox autour du point (20x20 pixels)\n    const padding = 10;\n    const bbox = [\n      [point.x - padding, point.y - padding],\n      [point.x + padding, point.y + padding]\n    ];\n\n    const features = map.queryRenderedFeatures(bbox, {\n      layers: ['building']\n    });\n\n    if (features.length > 0) {\n      const building = features[0];\n      const geometry = building.geometry;\n\n      if (geometry) {\n        map.addSource('highlighted-building', {\n          type: 'geojson',\n          data: {\n            type: 'Feature',\n            geometry,\n            properties: {\n              height: building.properties?.height || 30,\n              min_height: building.properties?.min_height || 0\n            }\n          }\n        });\n\n        map.addLayer({\n          id: 'highlighted-building-layer',\n          type: 'fill-extrusion',\n          source: 'highlighted-building',\n          paint: {\n            'fill-extrusion-color': '#ff0000',\n            'fill-extrusion-height': ['get', 'height'],\n            'fill-extrusion-base': ['get', 'min_height'],\n            'fill-extrusion-opacity': 1\n          }\n        });\n      }\n    } else {\n      console.warn(\"Aucun bâtiment trouvé dans cette zone.\");\n    }\n  });\n};\n\ndocument.head.appendChild(script);\n","type":"custom-js","disabled":true}},"trigger":"_wwOnMounted","firstAction":"04e641f9-c390-453a-8b0b-e3840cd275fa","triggerConditions":null}]},"content":{"default":{"source":"<div id=\"map4\" style=\"width: 900px; min-height: 100px; height: 500px;\"></div>\n","refreshOnResize":false}},"name":null,"wwObjectBaseId":"6d692ca2-6cdc-4805-aa0c-211102f335d0","libraryComponentBaseId":null,"parentSectionId":"051bb0da-1be3-4717-a38c-677b45b2f1